@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .

<https://example.com/dataset/1> a dcat:Dataset ;
    dcterms:title "Example Dataset"@en ;
    dcterms:description "This is an example dataset in Turtle format for testing."@en ;
    dcat:keyword "test", "example" ;
    dcterms:license <http://creativecommons.org/licenses/by/4.0/> ;
    dcat:contactPoint [
        a vcard:Organization ;
        vcard:fn "Example Organization" ;
        vcard:hasEmail <mailto:contact@example.com>
    ] ;
    dcterms:publisher [
        a foaf:Organization ;
        foaf:name "Example Publisher"
    ] ;
    dcat:distribution <https://example.com/dataset/1/distribution/1> .

<https://example.com/dataset/1/distribution/1> a dcat:Distribution ;
    dcterms:title "CSV Distribution"@en ;
    dcat:accessURL <https://example.com/data.csv> ;
    dcat:downloadURL <https://example.com/download/data.csv> ;
    dcterms:format "CSV" ;
    dcat:mediaType "text/csv" .